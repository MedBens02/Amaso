# Development Environment Configuration Template
# Copy this file to .env in the same directory as docker-compose.yml for development

# Git Repository Settings
REPO_URL=https://github.com/MedBens02/Amaso.git
BRANCH=main

# Database Configuration (Development)
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=amaso
MYSQL_USER=amaso_user
MYSQL_PASSWORD=password

# Application Environment
NODE_ENV=development
APP_ENV=local
APP_DEBUG=true

# Service Configuration
FRONTEND_PORT=3001
BACKEND_PORT=8001
DATABASE_PORT=3307